/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in = a, out = i1);
    Not(in = b, out = i2);
    Nand(a=i1, b=i2, out=out);
}